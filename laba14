import random
def Func():
      global mas
      global n
      if n < 0:
            return 1
      else:
            if len(set(mas)) != len(mas):
                  return False
            else:
                  return True
            return Func()
mas=[]
n=int(input("Введите размер массива >>> "))
a=int(input("Введите начало >>> "))
b=int(input("Введите конец >>> "))
for i in range(n):
      mas.append(random.randint(a,b))
print(mas)
print(Func())
