n = 20 
ys = [] 
for i in range(1,n+1): 
    ys.append(int(input(f'ученик №{i}: ' ))) 
print('кол во \'5\' : ' , ys.count(5)) 
print('кол во \'4\' : ' , ys.count(4)) 
print('кол во \'3\' : ' , ys.count(3)) 
print('кол во \'2\' : ' , ys.count(2)) 
b = sum(ys) 
print(b/n) 
p = ys.count(5) + ys.count(4)  
print((p/n)*100,"%")
